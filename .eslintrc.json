{
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "prettier",
    "plugin:jest/recommended"
  ],
  "rules": {
    "no-console": 1
  },
  "plugins": ["import", "jest"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest/globals": true
  }
}
